{
  env: {
    env_type: gym_minigrid,
    env_task: MiniGrid-Dynamic-Obstacles,
    grid_size: 8,
    action_size: 4,
    rgb_image: true,
    tile_size: 4,
    goal_type: from_buffer,
    from_buffer_choice_params: {
        warmup_steps: 1000,
        buffer_size: 100000,
        unachieved_prob: 0.5
        unachieved_buffer_size: 100,
    },
    random_network_distillation_reward: {
      device: "cpu",
      gamma: 0.999,
      target: {
        state_encoder_type: simple_cnn,
        n_channels: [16],
        kernel_sizes: [4],
        strides: [4],
        head: {
            hidden_size: 64
        }
      },
      predictor: {
        state_encoder_type: simple_cnn,
        n_channels: [16],
        kernel_sizes: [4],
        strides: [4],
        head: {
            hidden_size: 64
        }
      }
    },
    goal_achieving_criterion: position,
    ssim_network_params: {
        path: artifacts/models/minigrid_ssim.p,
        device: "cpu",
        threshold: 0.35
    },
    video_path: artifacts/video/
  },

  worker: {
    state_encoder_type: simple_cnn,
    n_channels: [16, 16],
    kernel_sizes: [4, 3],
    strides: [4, 1],
    paddings: [0, 1],
    head: {
        hidden_size: 64
    }
  },

  master: {
    state_encoder_type: simple_cnn,
    n_channels: [16, 16],
    kernel_sizes: [4, 3],
    strides: [4, 1],
    paddings: [0, 1],
    head: {
        hidden_size: []
    },
    emb_size: 32
  },

  agent: {
    device: "cpu",
    clip_param: 0.2,
    ppo_epoch: 4,
    num_mini_batch: 4,
    value_loss_coef: 0.5,
    entropy_coef: 0.01,
    lr: 0.001,
    eps: 1e-5,
    max_grad_norm: 0.5,
    gamma: 0.99,
    gae_lambda: 0.95
  },

  training: {
    reward: fair_goal_achievement,
    n_env_steps: 2000000,
    n_steps: 128,
    n_processes: 16,
    verbose: 1,
  },

  seed: 42,

  outputs: {
    path: artifacts/models/minigrid_worker_ssim.p,
  }
}
