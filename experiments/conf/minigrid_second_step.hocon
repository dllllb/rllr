{
  env: {
    env_type: gym_minigrid,
    env_task: MiniGrid-Dynamic-Obstacles,
    grid_size: 8,
    action_size: 3,
    rgb_image: true,
    tile_size: 4,
    video_path: artifacts/video/
  },

  worker_agent: {
    path: "artifacts/models/minigrid_worker.p",
    device: "cuda:0"
  }

  master: {
    state_encoder_type: simple_cnn,
    n_channels: [6, 16],
    kernel_sizes: [4, 3],
    max_pools: [4, 1],
    head: {
        hidden_size: 64
    }
  }

  master_agent: {
    buffer_size: 1000000,
    batch_size: 512,
    noise_decay: 0.995,
    start_noise: 1,
    min_noise: 0.01,
    explore: true,
    update_step: 4,
    device: "cuda:0"
  }

  training: {
    n_episodes: 1000,
    verbose: 100,
  },

  seed: 42,

  outputs: {
      path: artifacts/models/minigrid_master.p,
  }
}
